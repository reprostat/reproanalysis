<?xml version="1.0" encoding="utf-8"?>
<rap>
    <!--  The domain and the modality of a generic module is determined by the main input, which is expected to be the last inputstream -->
    <header domain='?' desc='SPM smooth' modality='MRI'/>

    <hpc>
        <timeBase>0.5</timeBase>
        <memoryBase>1</memoryBase>
    </hpc>

    <permanenceofoutput>2</permanenceofoutput>

    <settings>
        <FWHM>5</FWHM>
    </settings>

    <inputstreams>
        <stream domain='fmrirun' isrenameable='1' tobemodified='0'>fmri</stream>
    </inputstreams>

    <outputstreams>
        <stream domain='fmrirun' isrenameable='1'>fmri</stream>
    </outputstreams>
</rap>
