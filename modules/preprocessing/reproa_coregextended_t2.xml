<?xml version="1.0" encoding="utf-8"?>
<rap>
    <header mfile='reproa_coregextended' domain='subject' desc='SPM coregistration (extended a la DCCN)' modality='MRI'/>

    <hpc>
        <timeBase>1</timeBase>
        <memoryBase>1</memoryBase>
    </hpc>

    <permanenceofoutput>-1</permanenceofoutput>

    <settings>
      <target desc="name of the registration target stream" options='structural|t2' ui='optionlist'>structural</target>
      <reorienttotemplate desc="reorient all images to the MNI space">1</reorienttotemplate>
    </settings>

    <inputstreams>
        <stream domain="subject">structural</stream>
        <stream domain="subject">t2</stream>
    </inputstreams>

    <outputstreams>
        <stream domain="subject">structural</stream>
        <stream domain="subject">t2</stream>
    </outputstreams>
</rap>
